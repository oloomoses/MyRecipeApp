<% if obj.errors.any? %>
  <div class = 'row'>
    <div class = 'col-md-8 col-md-offset-2 alert alert-danger'>
      <h4>Correct the following Errors!</h4>
      <% obj.errors.full_messages.each do |msg| %>
        <ul>
          <li><%= msg %></li>
        </ul>
      <% end %>
    </div>
  </div>
<% end %>